{
  "crons": [
    {
      "path": "/api/cron.ts",
      "schedule": "*/10 * * * *"
    }
  ],
  "functions": {
    "api/cron.ts": {
      "maxDuration": 60
    }
  },
  "rewrites": [
    {
      "source": "/api/cron.ts",
      "destination": "/api/cron.ts"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/index.ts"
    }
  ]
}
