{
  "crons": [
    {
      "path": "/api/cron",      
      "schedule": "*/10 * * * *"
    }
  ],
  "functions": {
    "api/cron.ts": {           
      "maxDuration": 60
    }
  },
  "rewrites": [
    {
      "source": "/api/cron",    
      "destination": "/api/cron" 
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/index.ts"
    }
  ]
}
